<!DOCTYPE html>
<html>
<head>
  <title>Baseapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="site_wrapper">

  <div class="container">

    <div id="header">
      <%= render 'layouts/header' %>
    </div>
    <!-- /#header -->

    <div class="main" id="main" role="main">
      <%= yield %>
    </div>
    <!-- /#main -->

    <div class="contentinfo" id="contentinfo" role="contentinfo">

      <div id="footer">

      </div>
      <!-- /#footer -->

    </div>
    <!-- /#contentinfo -->

  </div>
  <!-- /.container -->


</div>
<!-- /#site_wrapper -->


</body>
</html>
